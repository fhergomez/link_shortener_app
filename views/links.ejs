<h1>Link Leaderboard</h1>

<a href="/" class="btn btn-success">Add a link</a>

<% orderedLinks.forEach(function(link) { %>
<div class="well">
  <div class="row">
    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
      <h3><%= link.clickCount %></h3>
    </div>
    <div class="col-xs-11 col-sm-11 col-md-11 col-lg-11">
      <h3><a href="/<%= hashing(link.id) %>" target="_blank">http://fhergomez/<%= hashing(link.id) %></a></h4>
    </div>
  </div>
</div>
<% }); %>
